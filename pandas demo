        "def find_day(month):\n",
        "  match month:\n",
        "    case 1:\n",
        "      print(\"january\")\n",
        "    case 2:\n",
        "      print(\"feburary\")\n",
        "    case 3:\n",
        "      print(\"march\")\n",
        "    case 4:\n",
        "      print(\"april\")\n",
        "    case 5:\n",
        "      print(\"may\")\n",
        "    case _:\n",
        "      print(\"invalid month\")\n",
        "find_day(8)\n",
        "\n",
        "\n"
      ],
